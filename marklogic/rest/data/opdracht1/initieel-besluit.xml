		  <Inhoud>
		    <Tussenkop>Inijkingstabel voor de energieklassen</Tussenkop>
		    <table colsep="1" frame="all" rowsep="1" tabstyle="zebra1" pgwide="0" wId="gm0307_1__h_1__art_1-1__para_3__table_1" eId="h_1__art_1-1__para_3__table_1">
